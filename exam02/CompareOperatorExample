package sec03.exam01;

public class CompareOperatorExample {

	public static void main(String[] args) {
		int x = 10;
		int y = 10;
		boolean result1 = (x == y);
		System.out.println(result1);
		
		boolean result2 = (x >= y);
		System.out.println(result2);
		
		boolean result3 = (x != y);
		System.out.println(result3);

	}

}
