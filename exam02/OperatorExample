package sec03.exam01;

public class OperatorExample {

	public static void main(String[] args) {
		int num = 100;
		int result1 = +num;
		int result2 = -num;
		System.out.println(result1 + "\n");
		System.out.println(result2 + "\n");
		
		num++;
		num--;
		++num;
		System.out.println(num);
		

	}

}
